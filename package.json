{
  "name": "saaskit",
  "private": true,
  "version": "0.0.0",
  "packageManager": "pnpm@10.13.1",
  "scripts": {
    "dev": "node scripts/dev-banner.js && concurrently --names \"app,web,api\" --prefix-colors \"cyan,magenta,yellow\" \"pnpm --filter app dev\" \"pnpm --filter web dev\" \"pnpm --filter api dev\"",
    "dev:app": "pnpm --filter app dev",
    "dev:web": "pnpm --filter web dev",
    "dev:api": "pnpm --filter api dev",
    "build": "turbo run build",
    "lint": "turbo run lint",
    "clean": "turbo run clean"
  },
  "devDependencies": {
    "@better-auth/cli": "^1.3.34",
    "@typescript-eslint/eslint-plugin": "^8.47.0",
    "@typescript-eslint/parser": "^8.47.0",
    "concurrently": "^9.2.1",
    "cross-env": "^7.0.3",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^9.1.2",
    "eslint-plugin-react-hooks": "^5.2.0",
    "prettier": "^3.6.2",
    "rimraf": "^5.0.10",
    "tsup": "^8.3.0",
    "tsx": "^4.19.0",
    "turbo": "^2.5.4",
    "typescript": "^5.4.0",
    "typescript-eslint": "^8.47.0"
  }
}
