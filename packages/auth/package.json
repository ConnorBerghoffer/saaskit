{
  "name": "@connorberghoffer/saaskit-auth",
  "version": "1.0.0",
  "description": "Complete authentication package with Better Auth, animated UI components, and full auth flows",
  "type": "module",
  "main": "dist/index.cjs",
  "types": "dist/index.d.cts",
  "files": [
    "dist",
    "README.md"
  ],
  "keywords": [
    "auth",
    "authentication",
    "better-auth",
    "react",
    "oauth",
    "email-password"
  ],
  "author": "",
  "license": "UNLICENSED",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.cts",
      "default": "./dist/index.cjs"
    },
    "./server": {
      "types": "./dist/server/index.d.cts",
      "default": "./dist/server/index.cjs"
    },
    "./client": {
      "types": "./dist/client/index.d.cts",
      "default": "./dist/client/index.cjs"
    },
    "./components": {
      "types": "./dist/components/index.d.cts",
      "default": "./dist/components/index.cjs"
    },
    "./hooks": {
      "types": "./dist/hooks/index.d.cts",
      "default": "./dist/hooks/index.cjs"
    },
    "./providers": {
      "types": "./dist/providers/index.d.cts",
      "default": "./dist/providers/index.cjs"
    }
  },
  "scripts": {
    "build": "tsup",
    "clean": "rimraf dist",
    "lint": "eslint src --ext ts,tsx",
    "prepublishOnly": "pnpm build"
  },
  "peerDependencies": {
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "dependencies": {
    "@connorberghoffer/saaskit-config": "^1.0.0",
    "@connorberghoffer/saaskit-db": "^1.0.0",
    "better-auth": "^1.0.0",
    "framer-motion": "^11.0.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0"
  }
}
